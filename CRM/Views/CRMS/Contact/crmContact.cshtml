    function SaveContact() {

        var obj = {};
        if ($("#hdnContact_Id").val() == "") {
            obj.Action = "Add";
        }
        else {
            obj.Action = "Update";
        }

        if (cfirstname.GetText() == "") {
            jAlert('Please Enter a valid First Name to proceed.', 'Alert');
            return;
        }

        if (clastname.GetText() == "") {
            jAlert('Please Enter a valid Last Name to proceed.', 'Alert');
            return;
        }

        obj.Contact_Id = $("#hdnContact_Id").val();
        obj.firstname = cfirstname.GetText();
        obj.lastname = clastname.GetText();
        obj.Unique_Id = cshortname.GetText();

        obj.job_title = $("#jobResponsibilityID").val();
        obj.jobResponsibilityID = 0;
        obj.email = cemail.GetText();
        obj.Business_phone = cBusiness_phone.GetText();
        obj.Mobile = cMobile.GetText();
        obj.Fax = cFax.GetText();
        obj.PrefferedMethodofContactID = $("#PrefferedMethodofContactID").val();
        obj.WonerID = $("#WonerID").val();
        obj.Status_Id = $("#Status_Id").val();
        obj.SourceID = $("#SourceID").val();
        obj.RatingID = $("#RatingID").val();
        obj.Gender = $("#ddlGender").val();
        obj.Account_ID = $("#Account_ID").val();
        obj.maritalstatus_ID = $("#maritalstatus_ID").val();
        obj.Spouce_Name = cSpouce_Name.GetText();
        obj.BirthDay = cBirthDay.GetDate();
        obj.Anniversary = cAnniversary.GetDate();
        obj.Credit_Limit = cCredit_Limit.GetValue();
        obj.PaymentTermID = $("#PaymentTermID").val();
        obj.Credit_holdId = $("#ddlCredit_hold").val();
        obj.Originating_Lead = cOriginating_Lead.GetText();
        obj.Last_Campaign_Date = cLast_Campaign_Date.GetDate();
        obj.Marketing_Materials = $("#mm").val();
        obj.AssignedID = $("#AssignedID").val();
        obj.ShippingMethodID = $("#ShippingMethodID").val();
        obj.Freight_Terms = cFreight_Terms.GetText();

        obj.Address1 = cAddress1.GetText();
        obj.Address2 = cAddress2.GetText();
        obj.Address3 = cAddress3.GetText();
        obj.LandMark = cLandMark.GetText();
        obj.pin = cpin.GetText();

        var leads = gridLookuplead.GetSelectedKeyFieldValues();
        obj.crmleads_id = leads.join();


        // Mantis Issue 21677,21676,23104 (03/06/2021)
        var opportunity = gridLookupOpportunity.GetSelectedKeyFieldValues();
        obj.crmoportunity_id = opportunity.join();
        // End of Mantis Issue 21677,21676,23104 (03/06/2021)


        LoadingPanel.Show();
        $.ajax({
            type: "POST",
            url: "/CRMContact/SaveContact",
            data: JSON.stringify(obj),
            async: true,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                console.log(response);
                jAlert(response);
                gridcrmContact.Refresh();
                gridcrmContact.Refresh();
                $("#addContact").modal('hide');
                LoadingPanel.Hide();
            },
            error: function (response) {
                jAlert("Please try again later");
                LoadingPanel.Hide();
            }
        });

    }

    function EditClick(id) {
        $("#hdnContact_Id").val(id);
        var obj = {};
        obj.Contact_Id = id;
        LoadingPanel.Show();
        $.ajax({
            type: "POST",
            url: "/CRMContact/EditContact",
            data: JSON.stringify(obj),
            async: true,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                LoadingPanel.Hide();
                cfirstname.SetText(response.firstname);
                clastname.SetText(response.lastname);
                $("#jobResponsibilityID").val(response.job_title);
                cemail.SetText(response.email);
                cshortname.SetText(response.Unique_Id);
                cshortname.SetEnabled(false);
                cBusiness_phone.SetText(response.Business_phone);
                cMobile.SetText(response.Mobile);
                cFax.SetText(response.Fax);
                $("#PrefferedMethodofContactID").val(response.PrefferedMethodofContactID);
                $("#WonerID").val(response.WonerID);
                $("#Status_Id").val(response.Status_Id);
                $("#SourceID").val(response.SourceID);
                $("#RatingID").val(response.RatingID);
                $("#Account_ID").val(response.Account_ID);
                $("#mm").val(response.Marketing_Materials);

                $("#ddlGender").val(response.Gender);
                $("#maritalstatus_ID").val(response.maritalstatus_ID);
                cSpouce_Name.SetText(response.Spouce_Name);
                if (response.BirthDay != null)
                    cBirthDay.SetDate(new Date(parseInt(response.BirthDay.substr(6))));
                if (response.Anniversary != null)
                    cAnniversary.SetDate(new Date(parseInt(response.Anniversary.substr(6))))
                cCredit_Limit.SetValue(response.Credit_Limit);
                $("#PaymentTermID").val(response.PaymentTermID);
                $("#ddlCredit_hold").val(response.Credit_holdId);
                cOriginating_Lead.SetText(response.Originating_Lead);
                if (response.Last_Campaign_Date != null)
                    cLast_Campaign_Date.SetDate(new Date(parseInt(response.Last_Campaign_Date.substr(6))))
                $("#AssignedID").val(response.AssignedID);
                $("#ShippingMethodID").val(response.ShippingMethodID);
                cFreight_Terms.SetText(response.Freight_Terms);

                cAddress1.SetText(response.Address1);
                cAddress2.SetText(response.Address2);
                cAddress3.SetText(response.Address3);
                cLandMark.SetText(response.LandMark);
                cpin.SetText(response.pin);
                ccity.SetText(response.city);
                cstate.SetText(response.state);
                ccountry.SetText(response.country);
                cOriginating_Lead.SetText(response.Originating_Lead);

                gridLookuplead.GetGridView().SelectRowsByKey(response.LEAD_LIST);
                // Mantis Issue 21677,21676,23104 (03/06/2021) [ the setTimeout is used to resolve refresh issue in the control. Refresh was not happening]
                setTimeout(function () { gridLookupOpportunity.GetGridView().SelectRowsByKey(response.oppids); }, 1200);
                gridLookupOpportunity.Refresh
                // End of Mantis Issue 21677,21676,23104 (03/06/2021)

                $("#addContact").modal('show');

            },
            error: function (response) {
                jAlert("Can not Edit");
                LoadingPanel.Hide();
            }
        });
    }

