<script>

    $(document).ready(function () {
        ctxtQuantityAdd.SetText(0);
        ctxtRateAdd.SetText(0);
        ctxtTotalAdd.SetText(0);
        GetObjectID('hdfProductIDAdd').value = 0;
        $("#ddlFrequency").val('');
        ctxtProductAdd.SetText('');
        ctxtRemarksAdd.SetText('');
    });

    function ProductButnClickAdd(s, e) {
        $('#ProductModelAdd').modal('show');
    }

    function Product_KeyDownAdd(s, e) {
        if (e.htmlEvent.key == "Enter") {
            $('#ProductModelAdd').modal('show');
        }
    }

    function AddActivityProductDetailsAdd() {


        var Quantity = ctxtQuantityAdd.GetText();
        var Rate = ctxtRateAdd.GetText();
        var ProductID = GetObjectID('hdfProductIDAdd').value;
        var ProductName = ctxtProductAdd.GetText();
        var Remarks = ctxtRemarksAdd.GetText();
        var Frequency = $("#ddlFrequency").val();
        var totalAmount = ctxtTotalAdd.GetText();
        if (ProductName == '' && Quantity == 0 && Rate == 0) {
            jAlert('Select Product,Quantity & Rate');
        }
        else {
            if (EditFlagCRM != "" && EditFlagCRM != null) {
                for (var i = 0; i < ActivityProductAdd.length; i++) {
                    if (ActivityProductAdd[i].guid == EditFlagCRM) {

                        ActivityProductAdd[i].ProductId = ProductID;
                        ActivityProductAdd[i].Quantity = Quantity;
                        ActivityProductAdd[i].Rate = Rate;
                        ActivityProductAdd[i].Remarks = Remarks;
                        ActivityProductAdd[i].ProductName = ProductName;
                        ActivityProductAdd[i].Frequency = Frequency;
                        ActivityProductAdd[i].Amount = totalAmount;

                        MakeTableFromArrayObjectAdd(ActivityProductAdd);
                        return;

                    }
                }
                EditFlagCRM = null;
            }

            var guid = uuidCRM();

            var ActivityProductboj = {};
            ActivityProductboj.guid = guid;
            ActivityProductboj.Quantity = Quantity;
            ActivityProductboj.Rate = Rate;
            ActivityProductboj.ProductId = ProductID;
            ActivityProductboj.Remarks = Remarks;
            ActivityProductboj.ProductName = ProductName;
            ActivityProductboj.Frequency = Frequency;
            ActivityProductboj.Amount = totalAmount;

            ActivityProductAdd.push(ActivityProductboj);


            MakeTableFromArrayObjectAdd(ActivityProductAdd);
        }
    }

    var ModuleNameActivityProductAdd = 'ACPRD';

    function MakeTableFromArrayObjectAdd(arr) {
        if (ModuleNameActivityProductAdd == 'ACPRD') {
            var str = "";
            for (var i = 0; i < arr.length; i++) {
                var sl = i + 1;
                str += "<tr>"
                str += "<td class='hide'>" + arr[i].guid + "</td>";
                str += "<td class='hide'>" + arr[i].ProductId + "</td>";
                str += "<td>" + sl + "</td>";
                str += "<td>" + arr[i].ProductName + "</td>";
                str += "<td>" + arr[i].Quantity + "</td>";
                str += "<td>" + arr[i].Rate + "</td>";
                str += "<td>" + arr[i].Amount + "</td>";
                str += "<td>" + arr[i].Remarks + "</td>";
                str += "<td>" + arr[i].Frequency + "</td>";

                str += "<td><a href='#' class='iconCD link' onclick='EditActivityProdAdd(" + JSON.stringify(arr[i].guid) + ")' ><img src='/assests/images/Edit.png' /></a>";
                str += "<a href='#' class='link' onclick='DeleteActivityProductAdd(" + JSON.stringify(arr[i].guid) + ")' ><img src='/assests/images/crs.png' /></a></td>";
                str += "</tr>"
            }
            $("#tbodyActivityProductAdd").html(str);

        }
    }

    function DeleteActivityProductAdd(uid) {
        for (var i = 0; i < ActivityProductAdd.length; i++) {
            if (ActivityProductAdd[i].guid == uid) {
                ActivityProductAdd.splice(i, 1);
            }
        }
        MakeTableFromArrayObjectAdd(ActivityProductAdd);
    }

    function EditActivityProdAdd(uid) {
        var EditActivityProdobj = $.grep(ActivityProductAdd, function (e) { return e.guid == uid });
        EditFlagCRM = uid;

        SetActivityProductOnEditAdd(EditActivityProdobj);
    }

    function ShowCRMProductsEditAdd(module_name, module_id) {
        crmProductModuleName = module_name;
        $.ajax({
            type: "POST",
            //url: "Url.Action("GetEmployeesTargetByCode", "EmployeesTarget")",
            url: "../CRMProducts/GetCRMProductsDetails",
            data: { Module_Name: module_name, Module_id: module_id },
            success: function (response) {

                ActivityProductAdd = response;
                MakeTableFromArrayObjectAdd(ActivityProductAdd);

            },
            error: function (response) {

            }
        });
    }

    function SetActivityProductOnEditAdd(EditActivityProdobj) {
        ctxtProductAdd.SetText(EditActivityProdobj[0].ProductName);
        GetObjectID('hdfProductIDAdd').value = EditActivityProdobj[0].ProductId;
        ctxtQuantityAdd.SetValue(EditActivityProdobj[0].Quantity);
        ctxtRateAdd.SetValue(EditActivityProdobj[0].Rate);
        ctxtRemarksAdd.SetText(EditActivityProdobj[0].Remarks);
        $("#ddlFrequency").val(EditActivityProdobj[0].Frequency);
        ctxtTotalAdd.SetValue(EditActivityProdobj[0].Amount);
    }

    function ProductkeydownAdd(e) {
        var OtherDetails = {}

        if ($.trim($("#txtProductSearchAdd").val()) == "" || $.trim($("#txtProductSearchAdd").val()) == null) {
            return false;
        }
        OtherDetails.SearchKey = $("#txtProductSearchAdd").val();

        if (e.code == "Enter" || e.code == "NumpadEnter") {
            var HeaderCaption = [];
            HeaderCaption.push("Product Name");
            HeaderCaption.push("Product Description");
            HeaderCaption.push("HSN/SAC");

            if ($("#txtProductSearchAdd").val() != "") {
                callonServer("../CRMActivity/GetcrmProducts", OtherDetails, "ProductTableAdd", HeaderCaption, "ProductIndexAdd", "SetProductAdd");
            }
        }
        else if (e.code == "ArrowDown") {
            if ($("input[ProductIndexAdd=0]"))
                $("input[ProductIndexAdd=0]").focus();
        }
    }

    function SetProductAdd(Id, Name) {
        //debugger;
        var key = Id;
        if (key != null && key != '') {
            $('#ProductModelAdd').modal('hide');
            ctxtProductAdd.SetText(Name);
            GetObjectID('hdfProductIDAdd').value = key;
            ctxtProductAdd.Focus();
        }
        else {
            ctxtProductAdd.SetText('');
            GetObjectID('hdfProductIDAdd').value = '';
        }
    }

    function LostFocusInRateAdd() {
        var Quantity = ctxtQuantityAdd.GetText();
        var Rate = ctxtRateAdd.GetText();
        var total = Quantity * Rate;
        ctxtTotalAdd.SetValue(total);
    }

    function LostFocusInTotalAdd() {
        var Quantity = ctxtQuantityAdd.GetText();
        var total = ctxtTotalAdd.GetText();
        var Rate = total / Quantity;

        ctxtRateAdd.SetValue(Rate);
    }
</script>
