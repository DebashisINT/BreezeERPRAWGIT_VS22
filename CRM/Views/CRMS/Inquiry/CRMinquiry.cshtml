<script type="text/javascript">

    function onInquirySourceSelectedIndexChanged() {
        var SourceID = $("#SourceID").val();
        var SourceName = $("#SourceID option:selected").text();
        var SourceType = "";

        ctxtInquiryValues.SetText("");
        GetObjectID('hdfInquiryValuesIDCRM').value = "";


        if (SourceName == "Existing Inquiry") {
            SourceType = "LD";
            ctxtInquiryValues.SetEnabled(true);
        }
        else if (SourceName == "Customer") {
            SourceType = "CL";
            ctxtInquiryValues.SetEnabled(true);
        }
        else if (SourceName == "Vendor" || SourceName == "Data Vendor") {
            SourceType = "DV";
            ctxtInquiryValues.SetEnabled(true);
        }
        else if (SourceName == "Salesmen/Agent") {
            SourceType = "AG";
            ctxtInquiryValues.SetEnabled(true);
        }
        else if (SourceName == "Influencer") {
            SourceType = "RA";
            ctxtInquiryValues.SetEnabled(true);
        }
        else if (SourceName == "Employee") {
            SourceType = "EM";
            ctxtInquiryValues.SetEnabled(true);
        }
        else if (SourceName == "Contacts") {
            SourceType = "CT";
            ctxtInquiryValues.SetEnabled(true);
        }
        else if (SourceName == "Existing Lead") {
            SourceType = "LD";
            ctxtInquiryValues.SetEnabled(true);
        }
        else {
            ctxtInquiryValues.SetEnabled(false);
        }

        if (SourceName == "Others") {
            SourceType = "Others";
            $("#divInquiryvalues").hide();
            $("#divOthersInquiryvalues").show();
        }
        else {
            $("#divInquiryvalues").show();
            $("#divOthersInquiryvalues").hide();
        }
        hdnSourceType.value = SourceType;
    }
    function Qualify(id, status) {
        if (status == 'Cancelled/Lost') {
            jAlert('Already Cancelled/Lost.');
        }
        else if (status == 'Qualify') {
            jAlert('Already Qualified.');
        }
        else {
            $("#hdnInquiry_Id").val(id);
            var obj = {};
            obj.InquiryCode = id;
            jConfirm('Confirm Qualify?', 'Confirmation Dialog', function (r) {
                if (r == true) {
                    $.ajax({
                        type: "POST",
                        url: "/Inquiry/QualifyInquiry",
                        data: JSON.stringify(obj),
                        async: true,
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (response) {
                            jAlert(response);
                            gridcrmInquiry.Refresh();
                            gridcrmInquiry.Refresh();
                        },
                        error: function (response) {
                            jAlert("Can not Qualified");
                            gridcrmInquiry.Refresh();
                            gridcrmInquiry.Refresh();
                            LoadingPanel.Hide();
                        }
                    });
                }
            });
        }

    }

    function Lost(id, status) {

        if (status == 'Qualify') {
            jAlert('Already Qualified. Can not modified.');
        }
        else {
