    function InquiryValueButnClick(s, e) {
        clear_PreviousCustdata();
        $('#InquiryValueModel').modal('show');
    }

    function InquiryValue_KeyDown(s, e) {
        clear_PreviousCustdata();
        if (e.htmlEvent.key == "Enter") {
            $('#InquiryValueModel').modal('show');
        }
    }
    function clear_PreviousCustdata() {
        // ctxtInquiryValues.SetText("");
        var OtherDetails = {}
        OtherDetails.SearchKey = 'abc123zzxx';
        OtherDetails.ContactType = $("#hdnSourceType").val();

        var HeaderCaption = [];
        HeaderCaption.push("Name");
        callonServer("../Inquiry/GetInquiryValues", OtherDetails, "InquiryValueTable", HeaderCaption, "InquiryValuesIndex", "SetInquiryValues");
    }
    function InquiryValueskeydown(e) {
        var OtherDetails = {}

        if ($.trim($("#txtInquiryValuesSearch").val()) == "" || $.trim($("#txtInquiryValuesSearch").val()) == null) {
            return false;
        }
        OtherDetails.SearchKey = $("#txtInquiryValuesSearch").val();
        OtherDetails.ContactType = $("#hdnSourceType").val();

        if (e.code == "Enter" || e.code == "NumpadEnter") {
            var HeaderCaption = [];
            HeaderCaption.push("Name");

            if ($("#txtInquiryValuesSearch").val() != "") {
                callonServer("../Inquiry/GetInquiryValues", OtherDetails, "InquiryValueTable", HeaderCaption, "InquiryValuesIndex", "SetInquiryValues");
            }
        }
        else if (e.code == "ArrowDown") {
            if ($("input[InquiryValuesIndex=0]"))
                $("input[InquiryValuesIndex=0]").focus();
        }
    }

    function SetInquiryValues(Id, Name) {
        debugger;
        var key = Id;
        if (key != null && key != '') {
            $('#InquiryValueModel').modal('hide');
            ctxtInquiryValues.SetText(Name);
            GetObjectID('hdfInquiryValuesIDCRM').value = key;
            ctxtInquiryValues.Focus();
        }
        else {
            ctxtInquiryValues.SetText('');
            GetObjectID('hdfInquiryValuesIDCRM').value = '';
        }
    }

</script>
