            success: function (data) {
                //console.log('AddTimesheet', data);
                var d = new Date();
                var mm = d.getMonth() + 1;
                var yyyy = d.getFullYear();
                fetTimesheetInfo(mm, yyyy);
                $('#applyTSEdit').modal('hide');
                //if (data.status == 200) {
                //    $("#applyWFH").modal("hide");
                //    getList();
                //    $.confirm({
                //        title: 'Success',
                //        content: 'WFH applied successfully',
                //        icon: 'fa fa-smile-o',
                //        theme: 'modern',
                //        closeIcon: true,
                //        animation: 'scale',
                //        type: 'blue',
                //    });

                //}
            }
        });
    }
    //End of rev Pratik

</script>
